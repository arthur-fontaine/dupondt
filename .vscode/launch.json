	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceFolder}/src/index.ts",
			"preLaunchTask": "tsc: build - tsconfig.json",
			"outFiles": ["${workspaceFolder}/bin/**/*.js"]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Instagram",
			"program": "${workspaceFolder}/src/utils/instagram.ts",
			"preLaunchTask": "tsc: build - tsconfig.json",
			"outFiles": ["${workspaceFolder}/bin/**/*.js"]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Simulation",
			"program": "${workspaceFolder}/src/utils/simulation.ts",
			"preLaunchTask": "tsc: build - tsconfig.json",
			"outFiles": ["${workspaceFolder}/bin/**/*.js"]
		}
	]
}
